    '''def Check(self):
        if self.Turno_Agua:
            return self.atacado(self.posicion_gyarados[0],self.posicion_gyarados[1])
        else:
            return self.atacado(self.posicion_charizard[0], self.posicion_charizard[1])
    def atacado(self,f, c):
        self.Turno = not self.Turno_Agua #cambiamos el turno
        enemigo_move= self.todos_mov()
        self.Turno = not self.Turno_Agua
        for movi in enemigo_move:
            if movi.eFila == f and movi.eCol == c:
                return True
        return False'''